{{ form_start(form) }}
	<div class="row">
		<div class="col-md-6"> {{ form_row(form.username) }}</div>
	</div>
	<div class="row">
		<div class="col-md-6"> {{ form_row(form.password) }}</div>
	</div>
	<div class="row">
		<div class="col-md-6">
			{{ form_widget(form) }}
			<button type="submit" class="btn btn-secondary">{{ button|default('Se connecter') }}</button>
		</div>
	</div>
{{ form_end(form) }}
